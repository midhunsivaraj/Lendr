        </activity>
        <activity android:name="com.rocsera.lendr.BrowseLendSelection" />
        <activity android:name="com.rocsera.lendr.SigninActivity" />
        <activity android:name="com.rocsera.lendr.SignupActivity" />
        <activity android:name="com.rocsera.lendr.MainActivity" />
    </application>

</manifest>